{
    "manifest_version": 2,
    "name": "Twitter Trending Section Blur",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": ["https://twitter.com/*"],
        "js": ["blur.js"],
        "run_at": "document_end"
      }
    ],
    "permissions": [
      "https://twitter.com/*"
    ]
  }